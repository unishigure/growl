{
  "name": "growl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "prisma migrate deploy",
    "migrate-dev": "prisma migrate dev",
    "generate": "prisma generate",
    "up": "docker compose -f \"docker/compose.yml\" up -d --build",
    "down": "docker compose -f \"docker/compose.yml\" down",
    "clean": "docker compose -f \"docker/compose.yml\" down --rmi all -v"
  },
  "dependencies": {
    "@prisma/client": "5.7.1",
    "cron": "^3.1.6",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
